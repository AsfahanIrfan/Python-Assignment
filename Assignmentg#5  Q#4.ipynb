    "def reverse(s): \n",
    "    return s[::-1] \n",
    "\n",
    "def isPalindrome(s):\n",
    "    reverse = s[::-1]   \n",
    "    if (s == reverse): \n",
    "        return True\n",
    "    return False\n",
    "    \n",
    "ans = isPalindrome(\"madam\")   \n",
    "if ans == True: \n",
    "    print(\"Yes\") \n",
    "else: \n",
    "    print(\"No\")"
   ]
