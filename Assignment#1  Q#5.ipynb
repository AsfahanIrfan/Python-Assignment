    "\n",
    "def user(fName,lName):\n",
    "    fullName = fName+\" \"+lName\n",
    "    reversedString=\"\"\n",
    "    i = len(fullName) \n",
    "    while i > 0: \n",
    "        reversedString += fullName[ i - 1 ]\n",
    "        i = i - 1 \n",
    "    print(reversedString)\n",
    "\n",
    "user(\"Asfahan\",\"Irfan\")"
   ]
